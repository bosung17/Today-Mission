package restavg;

import java.util.Arrays;
import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		double[] avg = new double[T];
		
		for (int i=0; i<T; i++) {
			int[] arr = new int[10];
			for (int j=0; j<10; j++) {
				arr[j] = sc.nextInt();
			}
			Arrays.sort(arr);

			int sum = 0;
			for (int j=1; j<9; j++) {
				sum += arr[j];
				avg[i] = sum/8;
			}
		}
		
		for (int i=0; i<T; i++) {
			System.out.println("#" + (i+1) + " " + avg[i]);
		}
		
	}
}
